// Limiting factor of int 32.
// used long long int to store reverse number.
// Time Complexity is 0(n).


class Solution {
public:
    int reverse(int x) {
        long long int rev=0;
        int a = abs(x);
        while(a!=0){
            
            rev= rev*10+ a%10;
            a = a/10;
        
            
        }
        if((rev > INT_MAX ) || (rev< INT_MIN)) return 0;
        else if(x>0) return rev;
        else return -(rev);
        
        
    }
};
