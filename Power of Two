// Time Complexity = 0(log(n)).
// Aasan sawal tha .. I understood it wrong.

class Solution {
public:
    bool isPowerOfTwo(int n) {
        if(n<=0) return false;
        //int count =0;
        while(n!=1){
            if(n%2 !=0)return false;
            n=n/2;
        }
        return true;
        

        
    }
};
